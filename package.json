{
    "name": "can-utils-node",
    "version": "0.1.0",
    "main": "dist/index.js",
    "repository": "https://github.com/nickhrsims/can-utils-node",
    "author": "Nicholas H.R. Sims",
    "license": "BSD0",
    "private": true,
    "scripts": {
        "check-types": "tsc",
        "transpile": "yarn babel src -d dist --extensions '.ts'",
        "clean": "/usr/bin/env bash -c 'if [[ -d dist ]]; then rm -r dist; fi'",
        "build": "yarn check-types && yarn clean && yarn transpile",
        "test": "jest -c jest.config.js"
    },
    "prettier": {
        "printWidth": 88,
        "tabWidth": 4,
        "useTabs": false,
        "semi": true,
        "singleQuote": true,
        "quoteProps": "as-needed",
        "trailingComma": "es5",
        "bracketSpacing": true,
        "arrowParens": "always",
        "requirePragma": false,
        "insertPragma": false,
        "proseWrap": "preserve",
        "htmlWhitespaceSensitivity": "css",
        "endOfLine": "lf",
        "embeddedLanguageFormatting": "off"
    },
    "devDependencies": {
        "@babel/node": "^7.14.7",
        "@types/jest": "^27.5.1",
        "jest": "^28.1.0",
        "ts-jest": "^28.0.2",
        "typescript": "^4.3.4"
    },
    "dependencies": {
        "@babel/cli": "^7.14.5",
        "@babel/core": "^7.14.6",
        "@babel/preset-env": "^7.14.7",
        "@babel/preset-typescript": "^7.14.5",
        "@types/node": "^15.12.4",
        "ajv": "^8.11.0",
        "babel-plugin-module-resolver": "^4.1.0"
    }
}
